{
  "builds": [
    {
      "src": "login/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "requirements.txt",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "staticfiles/",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "login/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    },
    "commands": [
      "pip install -r requirements.txt",
      "python manage.py collectstatic --noinput"
    ]
  }
}